{
  "name": "Darkam Theme",
  "type": "dark",
  "semanticHighlighting": false,
  "semanticTokenColors": {
    "variable": {
      "foreground": "#F8EAF8"
    },
    "variable.declaration": {
      "foreground": "#F8EAF8",
      "fontStyle": "italic"
    },
    "variable.constant": {
      "foreground": "#7ef091"
    },
    "variable.defaultLibrary": {
      "foreground": "#f07178"
    },
    "builtinConstant": {
      "foreground": "#e08adb"
    },
    "variable.readonly": {
      "foreground": "#e08adb",
      "fontStyle": "italic"
    },
    "variable.declaration.readonly": {
      "foreground": "#e08adb",
      "fontStyle": "italic"
    },
    "function": {
      "foreground": "#a5a1e9"
    },
    "class.typeHint.builtin": {
      "foreground": "#a5a1e9"
    },
    "string.regexp.quoted.multi.python.function": {
      "foreground": "#f5eef5"
    },
    "property": {
      "foreground": "#97b7fa"
    },
    "parameter": {
      "foreground": "#ffafaf"
    },
    "class": {
      "fontStyle": "bold",
      "foreground": "#7386d0"
    },
    "method": {
      "foreground": "#698cff"
    },
    "selfParameter": {
      "fontStyle": "bold italic",
      "foreground": "#79c0ff"
    },
    "magicFunction": {
      "foreground": "#79c0ff"
    }
  },
  "tokenColors": [
    {
      "name": "Inserted Text and Diff Headers",
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
