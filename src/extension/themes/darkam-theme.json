{
  "name": "Darkam Theme",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable.constant": {
      "foreground": "#7ef091"
    },
    "variable.defaultLibrary": {
      "foreground": "#f07178"
    },
    "variable.readonly": {
      "foreground": "#e83974",
      "fontStyle": "italic"
    },
    "property.declaration": {
      "foreground": "#f5eef5"
    }
  },
  "tokenColors": [
    {
      "name": "Markup and Diff",
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "background": "#04260f",
        "foreground": "#7ef091"
      }
    },
    {
      "name": "Markup Changed",
      "scope": ["markup.changed", "punctuation.definition.changed"],
      "settings": {
        "background": "#5a1e02",
        "foreground": "#ffa657"
      }
    },
    {
      "name": "Markup Italic",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#dedbe7"
      }
    },
    {
      "name": "Markup Bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#dedbe7"
      }
    },
    {
      "name": "Markup Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup Strikethrough",
      "scope": ["markup.strikethrough"],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Markup Heading",
      "scope": [
        "markup.heading entity.name",
        "markup.heading",
        "meta.separator"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#79c0ff"
      }
    },
    {
      "name": "Markup Ignored/Untracked",
      "scope": ["markup.ignored", "markup.untracked"],
      "settings": {
        "foreground": "#161b22",
        "background": "#79c0ff"
      }
    },
    {
      "name": "Markup Links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#72bfff",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Unmatched Bracket",
      "scope": ["brackethighlighter.unmatched", "message.error"],
      "settings": {
        "foreground": "#ffa198"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid.broken",
        "invalid.deprecated",
        "invalid.illegal",
        "invalid.unimplemented"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffa198"
      }
    },
    {
      "name": "Deleted",
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "background": "#490202",
        "foreground": "#ffa198"
      }
    },
    {
      "name": "Regex",
      "scope": [
        "source.regexp",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition",
        "string.regexp.character-class"
      ],
      "settings": {
        "foreground": "#a5cfff"
      }
    },
    {
      "name": "Links",
      "scope": ["constant.other.reference.link", "string.other.link"],
      "settings": {
        "foreground": "#ffa198",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Carriage Return",
      "scope": "carriage-return",
      "settings": {
        "fontStyle": "italic underline",
        "background": "#ff7b72",
        "foreground": "#f0f6fc",
        "content": "^M"
      }
    },
    {
      "name": "Unkown Changes",
      "scope": [
        "meta.block",
        "meta.embedded.expression",
        "meta.jsx.children",
        "meta.object.member",
        "meta.tag.attributes",
        "storage.modifier.import",
        "storage.modifier.package",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#dedbe7"
      }
    },
    {
      "name": "Meta Diff Range",
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#ca7ef0",
        "fontStyle": "bold"
      }
    },

    {
      "name": "Comments",
      "scope": [
        "brackethighlighter.angle",
        "brackethighlighter.curly",
        "brackethighlighter.quote",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.tag",
        "comment",
        "punctuation.definition.comment",
        "punctuation.definition.tag.xml",
        "string.comment"
      ],
      "settings": {
        "foreground": "#8b949e"
      }
    },
    {
      "name": ["Keywords", "Symbols"],
      "scope": [
        "keyword",
        "meta.tag.xml",
        "punctuation.section.embedded",
        "punctuation.separator.method.ruby",
        "punctuation.separator.namespace.xml",
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#ff7b72"
      }
    },
    {
      "name": "Python Keywords",
      "scope": [
        "constant.language.ruby",
        "entity.name.function.js",
        "markup.inline.raw",
        "meta.diff.header",
        "meta.module-reference",
        "meta.output",
        "meta.property-name",
        "string variable",
        "support.constant",
        "support.function.builtin.python",
        "support.variable"
      ],
      "settings": {
        "foreground": "#ff72b0"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string punctuation.section.embedded source"],
      "settings": {
        "foreground": "#a5cfff"
      }
    },
    {
      "name": "String Quotes",
      "scope": [
        "punctuation.definition.markdown",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.section.class.begin.python",
        "punctuation.section.class.begin.python",
        "punctuation.section.function.begin.python",
        "punctuation.section.function.end.python",
        "punctuation.section.function.lambda.begin.python",
        "punctuation.section.function.lambda.end.python",
        "punctuation.separator",
        "punctuation.separator.arguments.python",
        "punctuation.separator.colon.python",
        "punctuation.separator.dict.python",
        "punctuation.separator.element.python",
        "punctuation.separator.parameters.python",
        "punctuation.separator.period.python",
        "punctuation.separator.semicolon.python",
        "punctuation.support.type.property-name.begin.json",
        "punctuation.support.type.property-name.end.json"
      ],
      "settings": {
        "foreground": "#faa4c9"
      }
    },
    {
      "name": "Arguments Punctuation",
      "scope": [
        "punctuation.definition.arguments.begin.python",
        "punctuation.definition.arguments.end.python"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f0d388"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "entity.name.constant"],
      "settings": {
        "foreground": "#F0BC7E"
      }
    },
    {
      "name": ["Contants after =", "Contants", "Numbers", "Parameters"],
      "scope": [
        "constant.character.format.placeholder.other.python",
        "constant.language.python",
        "entity.name.tag",
        "markup.quote",
        "support.class.component",
        "support.type.exception.python",
        "support.type.property-name.json",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#7ef091"
      }
    },
    {
      "name": "Uppercase Contants",
      "scope": ["constant.other.caps.python"],
      "settings": {
        "foreground": "#eb5084"
      }
    },
    {
      "name": ["Constant Parameters"],
      "scope": [
        "entity.other.inherited-class",
        "meta.fstring.python",
        "meta.function-call.arguments.python",
        "source.python",
        "variable.language.special.self.python",
        "variable.other.block.ruby",
        "variable.parameter",
        "variable.parameter.function.language.python"
      ],
      "settings": {
        "foreground": "#f0bc7e"
      }
    },
    {
      "name": ["Language variables", "Namespaces", "Type Name"],
      "scope": [
        "constant.other.symbol.hashkey.ruby",
        "entity.name.namespace",
        "entity.other.attribute-name.namespace.xml",
        "variable.language.this.js"
      ],
      "settings": {
        "foreground": "#fdbaf2"
      }
    },
    {
      "name": "Read Write Variable (default variables)",
      "scope": ["variable.parameter.js", "variable.other.readwrite"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f5eef5"
      }
    },
    {
      "name": ["Declared Functions", "Attribute IDs"],
      "scope": [
        "entity.name.function.python",
        "markup.heading",
        "meta.function-call.generic.python",
        "punctuation.definition.heading",
        "support.function.kernel.ruby",
        "support.function.magic.python",
        "variable.other.object.property.js"
      ],
      "settings": {
        "foreground": "#d388f0"
      }
    },
    {
      "name": ["Imported Functions", "Attributes"],
      "scope": [
        "entity.name.function.ruby",
        "entity.other.attribute-name.localname.xml",
        "entity.other.attribute-name.xml",
        "meta.attribute.python"
      ],
      "settings": {
        "foreground": "#82aaff"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type.class.python",
        "support.class.ruby",
        "variable.other.class.js",
        "variable.other.class.ts"
      ],
      "settings": {
        "foreground": "#f0917e"
      }
    },
    {
      "name": "Testing Scope",
      "scope": [
        "dsadsaaaaaaaaaaaaaaaaaaaaaaaaaa",
        "dsadsaaaaaaaaaaaaaaaaaaaaaaaaaa",
        "dsadsaaaaaaaaaaaaaaaaaaaaaaaaaa"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#2fba89"
      }
    }
  ],
