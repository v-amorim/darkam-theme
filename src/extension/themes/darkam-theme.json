{
  "$schema": "../schema.json",
  "name": "Darkam Theme",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "builtinConstant": {
      "foreground": "#e08adb"
    },
    "variable.readonly": {
      "foreground": "#e08adb",
      "fontStyle": "italic"
    },
    "variable.declaration.readonly": {
      "foreground": "#e08adb",
      "fontStyle": "italic"
    },
    "function": {
      "foreground": "#a5a1e9"
    },
    "class.typeHint.builtin": {
      "foreground": "#a5a1e9"
    },
    "function.declaration": {
      "foreground": "#b0b9e5"
    },
    "class.declaration": {
      "fontStyle": "bold",
      "foreground": "#b0b9e5"
    },
    "method.declaration": {
      "foreground": "#b0b9e5"
    },
    "selfParameter": {
      "fontStyle": "bold italic",
      "foreground": "#79c0ff"
    },
    "magicFunction.declaration": {
      "foreground": "#79c0ff"
    },
    "string.regexp.quoted.multi.python.function": {
      "foreground": "#f5eef5"
    },
    "property": {
      "foreground": "#97b7fa"
    },
    "parameter": {
      "foreground": "#ffafaf"
    },
    "class": {
      "fontStyle": "bold",
      "foreground": "#7386D0"
    },
    "method": {
      "foreground": "#698cff"
    },
    "operator": {
      "foreground": "#ca5f71"
    },
    "variable.declaration": {
      "fontStyle": "italic"
    },
    "variable.constant": {
      "foreground": "#7ef091"
    },
    "variable.defaultLibrary": {
      "foreground": "#f07178"
    }
  },
  "tokenColors": [
    {
      "name": "Inserted Text and Diff Headers",
      "scope": [ "markup.inserted", "meta.diff.header.to-file", "punctuation.definition.inserted" ],
      "settings": {
        "background": "#04260f",
        "foreground": "#7ef091"
      }
    },
    {
      "name": "Ruby Language Boolean",
      "scope": [ "constant.language.boolean.ruby" ],
      "settings": {
        "foreground": "#7ef091"
      }
    },
    {
      "name": "Modified Text and Diff Indicators",
      "scope": [ "markup.changed", "punctuation.definition.changed" ],
      "settings": {
        "background": "#5a1e02",
        "foreground": "#ffa657"
      }
    },
    {
      "name": "Italic Text",
      "scope": [ "markup.italic", "meta.lambda-function.python" ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Bold Text",
      "scope": [ "markup.bold", "punctuation.definition.decorator.python" ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Bold and Italic Text Combination",
      "scope": [ "markup.bold markup.italic", "markup.italic markup.bold" ],
      "settings": {
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Underlined Text",
      "scope": [ "markup.underline" ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Strikethrough Text",
      "scope": [ "markup.strikethrough" ],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Headings and Python Magic Methods",
      "scope": [ "entity.name.section.markdown", "meta.separator", "support.function.magic.python" ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#79c0ff"
      }
    },
    {
      "name": "Python 'self' Parameter and Variables",
      "scope": [
        "punctuation.definition.variable.powershell",
        "support.variable.automatic.powershell",
        "support.variable.magic.python",
        "variable.language.special.cls.python",
        "variable.language.special.self.python",
        "variable.parameter.function.language.special.cls.python",
        "variable.parameter.function.language.special.self.python"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#79c0ff"
      }
    },
    {
      "name": "Ignored or Untracked Text in Version Control",
      "scope": [ "markup.ignored", "markup.untracked" ],
      "settings": {
        "foreground": "#161B22",
        "background": "#79c0ff"
      }
    },
    {
      "name": "Hyperlinks and URL References",
      "scope": [ "markup.underline.link" ],
